{
  "metadata": {
    "comment_samplers": "https://optuna.readthedocs.io/en/stable/reference/samplers.html",
    "comment_pruners": "https://optuna.readthedocs.io/en/stable/reference/pruners.html"
  },
  "data": {
    "class": "Optuna",
    "n_trials": 2,
    "work_path": "work",
    "storage": "sqlite:///work.db",
    "study": "container_simple",
    "copies": ["gmsh"],
    "sampler": "GridSampler",
    "sampler_kwargs": {
      "search_space": {
        "~~.gmsh.algorithm.2d": [2, 6],
        "~~.gmsh.algorithm.3d": [1, 4],
        "~~.gmsh.size.factor": [0.8, 1.3],
        "~~.gmsh.size.min": [0.1, 0.5],
        "~~.gmsh.size.max": [1.0, 5.0],
        "~~.gmsh.size.curvature": [50, 150],
        "~~.gmsh.size.boundary": [1, 2],
        "~~.gmsh.size.cast_iron": [0.1, 0.2],
        "~~.gmsh.size.filling": [0.1, 0.2]
      }},
    "pruner": "NopPruner",
    "pruner_kwargs": {},
    "sub_actions": [
      "/optimize/input.json",
      "/optimize/run.json",
      "/optimize/output.json"],
    "objectives": {
      "~~.output.mesh.metric.ige.min": "maximize"},
    "constraints": [
      "~~.output.mesh.jacobian.min.min.constraint"]
  }
}
